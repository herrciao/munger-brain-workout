{
  "buildCommand": "node validate.js && cd en && node validate.js",
  "outputDirectory": ".",
  "cleanUrls": true,
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        { "key": "Cache-Control", "value": "public, max-age=3600" }
      ]
    }
  ],
  "redirects": [
    {
      "source": "/en",
      "destination": "/en/index.html"
    }
  ]
}
